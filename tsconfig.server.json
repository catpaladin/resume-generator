{
  "compilerOptions": {
    "target": "ES2019",
    "lib": ["esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "esModuleInterop": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "incremental": false,
    "noEmit": false,
    "outDir": "./dist"
  },
  "include": ["api-proxy.ts"],
  "exclude": ["node_modules"]
}
